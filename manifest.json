{
  "manifest_version": 3,
  "name": "isThisOpen",
  "description": "Warns you if a tab you're about to open is already open.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "isThisOpenScript.js"
      ]
    }
  ]
}